{
  "version": "1.0.0-*",
  "authors": [ "Gilles Zunino" ],
  "description": "Microsoft Azure table storage secondary indices library for NetCore",

  "dependencies": {
    "NETStandard.Library": "1.6.1"
  },

  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },

  "buildOptions": {
    "define": [ "NETCORE" ],
    "outputName": "Echarde",
